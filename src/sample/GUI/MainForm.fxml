<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="594.0" prefWidth="481.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GUI.MainFormController">
    <children>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="Файл">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onSaveToFileClick" text="Сохранить..." />
                        <MenuItem mnemonicParsing="false" onAction="#onLoadFromFileClick" text="Загрузить..." />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Данные">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#onAddClick" text="Калькулятор" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <TableView fx:id="mainTable" prefHeight="546.0" prefWidth="481.0" VBox.vgrow="ALWAYS">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy></TableView>
    </children>
</VBox>
